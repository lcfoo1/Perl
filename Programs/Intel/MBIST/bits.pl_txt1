
my %Bin = (
                0 => 	"1111111111111111",
                1 => 	"1111111111111121",
                2 => 	"1111111111111211",
                3 => 	"1111111111112111",
                4 => 	"1111111111121111",
                5 => 	"1111111111211111",
                6 => 	"1111111112111111",
                7 => 	"1111111121111111",
                8 => 	"1111111211111111",
                9 => 	"1111112111111111",
		10 => 	"1111121111111111",
		11 => 	"1111211111111111",
		12 => 	"1112111111111111",
		13 => 	"1121111111111111",
		14 => 	"1211111111111111",
		15 => 	"2111111111111111"
	);

foreach my $i (0..16)
{
	#print "$Bin{$i}\n";
	my @char = split (//, $Bin{$i});
	my $Temp = "";
	foreach my $j (@char)
	{
		$Temp = $Temp . $j x 4;
	}
	my @NewTemps = split (//, $Temp);
	my $NewTemp = join(' ', @NewTemps);

	print "$NewTemp\n";
}
