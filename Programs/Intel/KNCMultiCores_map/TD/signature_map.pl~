
#my $CTV = "";
#my $CTV = "";
my $CTV = "5519911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991199119911991L99KJ99I75H99GF99ED99CB99A";

my @SC1 = ();
my @SC2 = ();
my @SC3 = ();
my @SC4 = ();
my $Cnt = 0;
my $TD1 = 0;#4;
my $TD2 = 59;#63;
my $GIDCnt = 0;
my $ShiftConst = 64;
my @CTVs = split('', $CTV);
my $Max = 252;
for (my $i=0; $i <= $Max; $i++)
{
	if ((($GIDCnt == $TD1) || ($GIDCnt == $TD2)) && ($Cnt == 2))
	{
		print "TD id = " . ($ShiftConst+$i) . ", position=" . ($ShiftConst+$Max-$i) .", value = $CTVs[$Max-$i], Cnt = $Cnt, gid = $GIDCnt\n";
		$Cnt = 0;
		$GIDCnt++;
	}
	elsif ($Cnt == 4)
	{
		print "Id = " . ($ShiftConst+$i) . ", position=" . ($ShiftConst+$Max-$i) .", value = $CTVs[$Max-$i], Cnt = $Cnt, gid = $GIDCnt\n";
		$Cnt = 0;
		$GIDCnt++;
	}


	if (!(($GIDCnt == $TD1) || ($GIDCnt == $TD2)))
	{
		push (@SC1, $ShiftConst+$i) if (0 == $Cnt);
		#push (@SC2, $ShiftConst+$i) if (1 == $Cnt);
		#push (@SC3, $ShiftConst+$i) if (2 == $Cnt);
		push (@SC4, $ShiftConst+$i) if (3 == $Cnt);
	}
	elsif (($GIDCnt == $TD1) || ($GIDCnt == $TD2))
	{
		push (@SC1, $ShiftConst+$i) if (0 == $Cnt);
		push (@SC4, $ShiftConst+$i) if (1 == $Cnt);
	}
	$Cnt++;
}

print join (',', @SC1) . "\n";
#print join (',', @SC2) . "\n";
#print join (',', @SC3) . "\n";
print join (',', @SC4) . "\n";

